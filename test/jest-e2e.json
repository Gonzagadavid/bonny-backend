{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@dogs/(.*)$": "<rootDir>/src/dogs/$1",
    "^@users/(.*)$": "<rootDir>/src/users/$1",
    "^@sponsorship/(.*)$": "<rootDir>/src/sponsorship/$1",
    "^@adoption/(.*)$": "<rootDir>/src/adoption/$1"
  }
}
